<reactiveUi:ReactiveUserControl
    x:Class="WolvenKit.Views.Editors.RedUlongEditor"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:WolvenKit.Views.Editors"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:reactiveUi="http://reactiveui.net"
    xmlns:shell="clr-namespace:WolvenKit.App.ViewModels.Shell;assembly=WolvenKit.App"
    xmlns:syncfusion="http://schemas.syncfusion.com/wpf"
    d:DesignHeight="100"
    d:DesignWidth="400"
    x:TypeArguments="shell:PropertyViewModel"
    mc:Ignorable="d">

    <Grid DataContext="{Binding RelativeSource={RelativeSource FindAncestor, AncestorType={x:Type local:RedUlongEditor}}}">

        <TextBox
            x:Name="TextBox"
            PreviewTextInput="NumberValidationTextBox"
            Text="{Binding Text, UpdateSourceTrigger=PropertyChanged}" />

    </Grid>
</reactiveUi:ReactiveUserControl>
